    private void jButton36ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        
        double t=Double.parseDouble(jTextField1.getText());
        double fact=1;
        while (t!=0){
            fact=fact*t;
            t--;
            
        }
        
            jTextField1.setText("");
            jTextField1.setText(jTextField1.getText()+fact);
    }                                         

