    private void jButton48ActionPerformed(java.awt.event.ActionEvent evt) {                                          

         num2 = Double.parseDouble(jTextField1.getText());
         if(opr=="+")
         {
             result = num1+num2;
             jTextField1.setText(Double.toString(result));
         
         }
         
         else if(opr=="-")
         {
             result = num1-num2;
             jTextField1.setText(Double.toString(result));
         
         }
         else if(opr=="*")
         {
             result = num1*num2;
             jTextField1.setText(Double.toString(result));
         
         }
         else if(opr=="/")
         {
             result = num1/num2;
             jTextField1.setText(Double.toString(result));
         
         }
         
        else if(opr=="%")
         {
             result = num1%num2;
             jTextField1.setText(Double.toString(result));
         
         }
         
         else if (opr="x^y")
             
         {
            
             for (int i=0;i<num2;i++){
         result =num1*num2;
         
         }
             jTextField1.setText(Double.toString(result));
         }
    }                                         

